    <gazebo>
        <plugin name="prop_cw_1_blade_1" filename="libLiftDragPlugin.so">
            <a0>0.3</a0>
            <alpha_stall>1.4</alpha_stall>
            <cla>4.2500</cla>
            <cda>0.10</cda>
            <cma>0.00</cma>
            <cla_stall>-0.025</cla_stall>
            <cda_stall>0.0</cda_stall>
            <cma_stall>0.0</cma_stall>
            <area>0.002</area>
            <air_density>1.2041</air_density>
            <cp>0.084 0 0</cp>
            <forward>0 1 0</forward>
            <upward>0 0 1</upward>
            <link_name>prop_cw_1</link_name>
