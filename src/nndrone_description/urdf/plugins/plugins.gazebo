    <xacro:property name="to_rad" value="0.0174533"/>
    <xacro:property name="xyz_ned_r" value="180.0"/>
    <xacro:property name="xyz_ned_p" value="0.0"/>
    <xacro:property name="xyz_ned_y" value="90.0"/>
    <gazebo>
        <plugin name="arducopter_plugin" filename="libArduPilotPlugin.so">
            <fdm_addr>127.0.0.1</fdm_addr>
            <fdm_port_in>$(arg fdm_port_in)</fdm_port_in>
            <fdm_port_out>$(arg fdm_port_out)</fdm_port_out>
            <!--
                Require by APM :
                Only change model and gazebo from XYZ to XY-Z coordinates
            -->
            <modelXYZToAirplaneXForwardZDown>0 0 0 3.141593 0 0</modelXYZToAirplaneXForwardZDown>
            <!-- <gazeboXYZToNED>0 0 0 3.141593 0 0</gazeboXYZToNED> -->
            <gazeboXYZToNED>0 0 0 ${to_rad*xyz_ned_r} ${to_rad*xyz_ned_p} ${to_rad*xyz_ned_y}</gazeboXYZToNED>
            <imuName>imu_sensor</imuName>
            <connectionTimeoutMaxCount>5</connectionTimeoutMaxCount>
            <control channel="0">
            <!--
                incoming control command [0, 1]
                so offset it by 0 to get [0, 1]
                and divide max target by 1.
                offset = 0
                multiplier = 838 max rpm / 1 = 838
                -->
                <type>VELOCITY</type>
                <offset>0</offset>
                <p_gain>0.20</p_gain>
                <i_gain>0</i_gain>
                <d_gain>0</d_gain>
                <i_max>0</i_max>
                <i_min>0</i_min>
                <cmd_max>2.5</cmd_max>
                <cmd_min>-2.5</cmd_min>
                <jointName>Rev1</jointName>
                <multiplier>838</multiplier>
                <controlVelocitySlowdownSim>1</controlVelocitySlowdownSim>
            </control>
            <control channel="1">
                <type>VELOCITY</type>
                <offset>0</offset>
                <p_gain>0.20</p_gain>
                <i_gain>0</i_gain>
                <d_gain>0</d_gain>
                <i_max>0</i_max>
                <i_min>0</i_min>
                <cmd_max>2.5</cmd_max>
                <cmd_min>-2.5</cmd_min>
                <jointName>Rev2</jointName>
                <multiplier>838</multiplier>
                <controlVelocitySlowdownSim>1</controlVelocitySlowdownSim>
            </control>
            <control channel="2">
                <type>VELOCITY</type>
                <offset>0</offset>
                <p_gain>0.20</p_gain>
                <i_gain>0</i_gain>
                <d_gain>0</d_gain>
                <i_max>0</i_max>
                <i_min>0</i_min>
                <cmd_max>2.5</cmd_max>
                <cmd_min>-2.5</cmd_min>
                <jointName>Rev3</jointName>
                <multiplier>-838</multiplier>
                <controlVelocitySlowdownSim>1</controlVelocitySlowdownSim>
            </control>
            <control channel="3">
