<?xml version="1.0" ?>
<robot name="nndrone" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:arg name="fdm_port_in" default="9002" />
  <xacro:arg name="fdm_port_out" default="9003" />

  <xacro:include filename="$(find nndrone_description)/urdf/materials.xacro" />
  <!-- <xacro:include filename="$(find nndrone_description)/urdf/nndrone.trans" /> -->
  <xacro:include filename="$(find nndrone_description)/urdf/nndrone.gazebo" />
  <xacro:include filename="$(find nndrone_description)/urdf/plugins/plugins.gazebo" />

  <link name="base_link">
     <inertial>
        <origin rpy="0 0 0" xyz="0.0 0.0 -0.0"/>
        <mass value="1.0"/>
        <inertia  ixx="0.00347019286099" ixy="0" ixz="0" iyy="0.0050603215505" iyz="0" izz="0.00730253830692" />    
      </inertial> 
  
    <visual>
      <origin rpy="0 0 -1.5708" xyz="0 0 -0.055"/>
      <geometry>
        <mesh filename="package://nndrone_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision> 
      <origin rpy="0 0 0" xyz="0 0 -0.0"/> 
      <geometry>
        <!-- <mesh filename="package://nndrone_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/> -->
          <box  size="0.18 0.2 0.1" />
      </geometry>
    </collision>
  </link>
  <link name="prop_cw_1">
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.02"/>
      <mass value="0.025"/>
      <inertia  ixx="9.75e-06" ixy="0" ixz="0" iyy="0.000166704" iyz="0" izz="0.000167604" />
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="-0.0985 -0.0695 -0.055"/>
      <geometry>
        <mesh filename="package://nndrone_description/meshes/prop_cw_1_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="yellow"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.02"/>      
      <geometry>
        <cylinder radius="0.1" length="0.005"/>
        <!-- <mesh filename="package://nndrone_description/meshes/prop_cw_1_1.stl" scale="0.001 0.001 0.001"/> -->
      </geometry> 
    </collision>
  </link>

  <link name="prop_ccw_3">
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.02"/>
      <mass value="0.025"/>
      <inertia  ixx="9.75e-06" ixy="0" ixz="0" iyy="0.000166704" iyz="0" izz="0.000167604" />
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0985 -0.0695 -0.055"/>
      <geometry>
        <mesh filename="package://nndrone_description/meshes/prop_ccw_3_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="yellow"/>
    </visual>  
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.02"/> 
      <geometry>
        <cylinder radius="0.1" length="0.005"/>
        <!-- <mesh filename="package://nndrone_description/meshes/prop_ccw_3_1.stl" scale="0.001 0.001 0.001"/> -->
      </geometry>
    </collision>
  </link>

  <link name="prop_cw_2">
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.02"/>
      <mass value="0.025"/>
      <inertia  ixx="9.75e-06" ixy="0" ixz="0" iyy="0.000166704" iyz="0" izz="0.000167604" />
    </inertial>
    <visual>
    <collision>
    </collision>>
      <origin rpy="0 0 0" xyz="0.0985 0.0869 -0.055"/>
      <geometry>
        <mesh filename="package://nndrone_description/meshes/prop_cw_2_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.02"/>      
      <geometry>
        <cylinder radius="0.1" length="0.005"/>
        <!-- <mesh filename="package://nndrone_description/meshes/prop_cw_2_1.stl" scale="0.001 0.001 0.001"/> -->
      </geometry>
    </collision>
  </link>

  <link name="prop_ccw_4">
    <inertial>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.02"/>
      <mass value="0.025"/>
      <inertia  ixx="9.75e-06" ixy="0" ixz="0" iyy="0.000166704" iyz="0" izz="0.000167604" />
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="-0.0985 0.0869 -0.055"/>
      <geometry>
        <mesh filename="package://nndrone_description/meshes/prop_ccw_4_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
    <origin rpy="0 0 0" xyz="0 0 0.02"/>      
      <geometry>
        <cylinder radius="0.1" length="0.005"/>
        <!-- <mesh filename="package://nndrone_description/meshes/prop_ccw_4_1.stl" scale="0.001 0.001 0.001"/> -->
      </geometry>
    </collision>
  </link>

  <link name="lidar_1">
    <inertial>
      <origin rpy="0 0 0" xyz="1.50702293634589e-17 0.001172866940571421 0.02762648466802698"/>
      <mass value="0.01"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="1e-06" iyz="-0.0" izz="1e-06"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="-0.0 -0.077 -0.0415"/>
      <geometry>
        <mesh filename="package://nndrone_description/meshes/lidar_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="black"/>
    </visual>

  </link>

  <link name="pi_camera_1">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.01"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="-0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 -0.06725 -0.036"/>
      <geometry>
        <mesh filename="package://nndrone_description/meshes/pi_camera_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="black"/>
    </visual>

  </link>

  <link name='imu_link'>
      <inertial>
        <origin rpy="0 0 0" xyz="0 0 0" />
        <mass value="0.15" />
        <inertia  ixx="0.00001" ixy="0" ixz="0" iyy="0.00002" iyz="0" izz="0.00002" />
      </inertial>

  </link>
  <link name='gps_link'>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0 " />
      <mass value="0.15" />
      <inertia ixx="0.00001" ixy="0.0" ixz="-0.0" iyy="0.00002" iyz="0.0" izz="0.00002" />
    </inertial>
  </link>


 <joint name="Rev1" type="revolute">
    <origin rpy="0 0 0" xyz="0.0695 -0.0985 0.001"/>
    <parent link="base_link"/>
    <child link="prop_cw_1"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.004"/>
    <limit lower="-1e+16" upper="1e+16" effort="5.0" velocity="2100"/>
  </joint>

  <joint name="Rev3" type="revolute">
    <origin rpy="0 0 0" xyz="0.0695 0.0985 0.001"/>
    <parent link="base_link"/>
    <child link="prop_ccw_3"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.004"/>
    <limit lower="-1e+16" upper="1e+16" effort="5.0" velocity="2100"/>
  </joint>

  <joint name="Rev2" type="revolute">
    <origin rpy="0 0 0" xyz="-0.0869 0.0985 0.001"/>
    <parent link="base_link"/>
    <child link="prop_cw_2"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.004"/>
    <limit lower="-1e+16" upper="1e+16" effort="5.0" velocity="2100"/>
  </joint>

  <joint name="Rev4" type="revolute">
    <origin rpy="0 0 0" xyz="-0.0869 -0.0985 0.001"/>
    <parent link="base_link"/>
    <child link="prop_ccw_4"/>
    <axis xyz="0 0 1"/>
    <dynamics damping="0.004"/>
    <limit lower="-1e+16" upper="1e+16" effort="5.0" velocity="2100"/>
  </joint>


  <joint name="lidar_joint" type="fixed">
    <origin rpy="0 0 1.5708" xyz="0.077 0.0 -0.015"/>
    <parent link="base_link"/>
    <child link="lidar_1"/>
  </joint>

  <joint name="rpi_cam_joint" type="fixed">
    <origin rpy="0 1.57 0" xyz="0.06725 0.0 -0.020"/>
    <parent link="base_link"/>
    <child link="pi_camera_1"/>
  </joint>

  <joint name="gps_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0" />
    <parent link="base_link" />
    <child link="gps_link" />
  </joint>

  <joint name="imu_joint" type="revolute">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <parent link="base_link" />
      <child link="imu_link" />
      <axis xyz="0 0 1"/>
      <dynamics damping="1.0" friction="0.0"/>
      <limit effort="0.0" lower="0.0" upper="0.0" velocity="0.0"/>
  </joint>

</robot>
