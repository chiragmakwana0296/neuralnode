    <axis xyz="0.0 0.0 1.0"/>
    <dynamics damping="0.005"/>
  </joint>

  <joint name="Rev4" type="continuous">
    <origin rpy="0 0 0" xyz="-0.0869 -0.0985 0.001"/>
