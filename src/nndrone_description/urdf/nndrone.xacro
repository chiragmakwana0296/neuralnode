    <origin rpy="0 0 1.5708" xyz="0.077 0.0 -0.015"/>
    <parent link="base_link"/>
    <child link="lidar_1"/>
  </joint>

  <joint name="rpi_cam_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.06725 0.0 -0.020"/>
