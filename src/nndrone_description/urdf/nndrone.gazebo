  <!-- <mu1>0.2</mu1> -->
  <!-- <mu2>0.2</mu2> -->
  <selfCollide>false</selfCollide>
</gazebo>

<gazebo reference="imu_link">
  <material>${sensor_color}</material>
  <!-- <mu1>0.2</mu1> -->
  <!-- <mu2>0.2</mu2> -->
  <selfCollide>false</selfCollide>
</gazebo>

<gazebo reference="Rev1">
  <implicitSpringDamper>true</implicitSpringDamper>
</gazebo>
