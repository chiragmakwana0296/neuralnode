  <material>${prop_color_front}</material>
  <!-- <mu1>0.2</mu1> -->
  <!-- <mu2>0.2</mu2> -->
  <selfCollide>false</selfCollide>
</gazebo>

<gazebo reference="prop_cw_2">
  <material>${prop_color_back}</material>
  <!-- <mu1>0.2</mu1> -->
  <!-- <mu2>0.2</mu2> -->
  <selfCollide>false</selfCollide>
</gazebo>

